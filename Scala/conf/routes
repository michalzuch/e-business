GET     /                   controllers.HomeController.index()
GET     /assets/*file        controllers.Assets.versioned(path="/public", file: Asset)

POST    /products           controllers.ProductController.create()
GET     /products           controllers.ProductController.getAll()
GET     /products/:id       controllers.ProductController.get(id: Int)
PUT     /products/:id       controllers.ProductController.update(id: Int)
DELETE  /products/:id       controllers.ProductController.delete(id: Int)

POST    /categories         controllers.CategoryController.create()
GET     /categories         controllers.CategoryController.getAll()
GET     /categories/:id     controllers.CategoryController.get(id: Int)
PUT     /categories/:id     controllers.CategoryController.update(id: Int)
DELETE  /categories/:id     controllers.CategoryController.delete(id: Int)

POST    /bags               controllers.BagController.create()
GET     /bags               controllers.BagController.getAll()
GET     /bags/:id           controllers.BagController.get(id: Int)
PUT     /bags/:id           controllers.BagController.update(id: Int)
DELETE  /bags/:id           controllers.BagController.delete(id: Int)